I = imread('ABC.jpeg');
subplot(2,2,1); imshow(I);
title('Original');
subplot(2,2,2); 
imshow(0.3*I(:,:,1) + 0.59*I(:,:,2) + 0.11*I(:,:,3), []); 
title('Grayscale (Formula)');
subplot(2,2,3); imshow(rgb2gray(I));
title('Grayscale (rgb2gray)');
subplot(2,2,4); imshow(im2bw(rgb2gray(I))); 
title('Binary');
